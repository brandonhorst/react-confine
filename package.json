{
  "name": "react-confine",
  "version": "0.0.1",
  "description": "React plugin for editing confine configurations (simple JSON schema)",
  "main": "lib/confine.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "confine": "^3.0.3",
    "lodash": "^3.3.1",
    "react": "^0.12.2"
  },
  "devDependencies": {
    "babel": "^4.6.6",
    "babelify": "^5.0.3",
    "browserify": "^9.0.3",
    "chai": "^2.1.0",
    "jsdom": "^4.0.1",
    "mocha": "^2.1.0",
    "normalize-css": "^2.3.1",
    "rimraf": "^2.3.0"
  },
  "scripts": {
    "test": "mocha --compilers jsx:babel/register",
    "prepublish": "npm run clean && npm run build && npm run demo:build",
    "demo:build": " mkdir -p demo/build && npm run build && cp demo/src/index.html demo/build && cp demo/src/style.css demo/build && browserify -t babelify demo/src/script.jsx -o demo/build/script.js",
    "demo": "npm run demo:build && open demo/build/index.html",
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib demo/build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonhorst/react-confine.git"
  },
  "keywords": [
    "react",
    "confine",
    "config",
    "configuration",
    "configure",
    "gui",
    "automatic",
    "generation",
    "autogenerate",
    "json",
    "schema",
    "jsonschema",
    "json-schema"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/react-confine/issues"
  },
  "homepage": "https://github.com/brandonhorst/react-confine"
}
